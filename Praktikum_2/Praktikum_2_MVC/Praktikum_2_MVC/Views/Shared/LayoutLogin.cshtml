@{ string title = "Studentenportal FH-Aachen";
    }
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
    "http://www.w3org/TR/html4/loose.dtd">
<html>
<head>
	<title>@title</title>
</head>
<body>

    <table border="0" width="100%">
        <tr>
            <th colspan="2"><h1>Willkommen zum Studentenportal</h1></th>
        </tr>
        <tr>
            <td>
                @RenderBody()
            </td>
            <td>
                <p><img src="~/35707-das-studium.jpg" name="http://lustich.de/bilder/autos/das-studium/" alt="http://lustich.de/bilder/autos/das-studium/"/></p>
            </td>
        </tr>
        <tr>
            <td>
                <form action="http://www.google.de/search" method="get" target="_blank">
                    <fieldset>
                        <legend>Durchsuche FH Seite (&uuml;ber Google)</legend>
                        <p>Suchbegriff: 
                        <input type="text" name="q" size="20" maxlength="40"/>
                        <input type="hidden" name="as_sitesearch" value="www.fh-aachen.de"/>
                        <input type="submit" name="submit" value="Los!"/>
                        </p>
                    </fieldset>
                </form>
            </td>
        </tr>
    </table>
    <a href="http://www.fh-aachen.de/" target="_blank">zur FH Startseite</a>

    <p>@DateTime.Now</p>
</body></html>